<resources>

    <style name="AppTheme.DeepPurple">
        <item name="android:statusBarColor">@color/theme_deep_purple_700</item>
        <item name="android:navigationBarColor">@color/black</item>
    </style>

    <style name="AppTheme.Translucent">
        <item name="android:windowBackground">@color/translucent_a80</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>

    <style name="AppTheme.Translucent.Full">
        <item name="android:windowFullscreen">true</item>
    </style>

</resources>
