<?xml version="1.0" encoding="utf-8"?><!-- 颜色的定义使用material_design,每种色值完全按照官网提供的来定义，
    命名规则：material_color_int[_alpha],material_color_int_颜色_饱和度_透明度
    颜色定义地址:http://design.1sters.com/material_design/style/color.html-->
<resources>
    <!-- black-->
    <color name="black">#000000</color>
    <!-- white-->
    <color name="white">#ffffff</color>
    <color name="primary_material_white">@color/white</color>
    <color name="primary_dark_material_white">#eeeeee</color>
    <color name="accent_material_white">#616161</color>
    <!-- gray -->
    <color name="material_gray_300">#e0e0e0</color>
    <color name="material_gray_500">#9e9e9e</color>
    <!-- brown -->
    <color name="material_brown_200">#efebe9</color>
    <color name="material_brown_700">#5d4037</color>
    <color name="primary_text_material_brown">#928460</color>
    <color name="material_brown_700_a50">#505d4037</color>
    <!-- red-->
    <color name="material_red_500">#e51c23</color>
    <color name="material_red_700">#d01716</color>
    <color name="primary_material_red">#e51c23</color>
    <color name="primary_dark_material_red">#d01716</color>
    <color name="accent_material_red">#ff5177</color>
    <!-- blue-->
    <color name="primary_material_blue">#03a9f4</color>
    <color name="primary_dark_material_blue">#0288d1</color>
    <color name="accent_material_blue">#40c4ff</color>
    <color name="material_blue_700">#0288d1</color>
    <color name="material_blue_500">#03a9f4</color>
    <!-- 圈子页消息提醒的字体颜色 -->
    <color name="remind_msg_pink">#fff8685c</color>
    <color name="light_gray">#ffa5a5a5</color>
    <!-- deep purple-->
    <color name="primary_material_deep_purple">@color/material_deep_purple_100</color>
    <color name="primary_dark_material_deep_purple">@color/material_deep_purple_100</color>
    <color name="accent_material_deep_purple">@color/material_deep_purple_100</color>
    <color name="material_deep_purple_100">#d1c4e9</color>

</resources>